<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="document_finder_service">
	<namespace>FOO</namespace>
    <entity name="DocumentInfo" local-service="true" remote-service="false">
        <column name="DocID" type="long" primary="true"></column>
        <column name="DocName" type="String"></column>
        <column name="DocGen" type="String"></column>
        <column name="DocFee" type="int"></column>
        <column name="DocSize" type="String"></column>
        <column name="DocPreUrl" type="String"></column>
        <column name="DocDownUrl" type="String"></column>
    </entity>

</service-builder>