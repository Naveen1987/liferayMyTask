<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="document_finder_service.model.DocumentInfo" />
	<class name="document_finder_service.model.impl.DocumentInfoImpl" table="FOO_DocumentInfo">
		<id name="DocID" type="long">
			<generator class="assigned" />
		</id>
		<property name="DocName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="DocGen" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="DocFee" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="DocSize" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="DocPreUrl" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="DocDownUrl" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>